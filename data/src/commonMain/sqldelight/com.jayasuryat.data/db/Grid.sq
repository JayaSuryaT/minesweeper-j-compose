CREATE TABLE Grid (
  rows INTEGER NOT NULL,
  columns INTEGER NOT NULL,
  totalMines INTEGER NOT NULL,
  startTime INTEGER NOT NULL,
  endTime INTEGER,
  grid TEXT NOT NULL
);

CREATE INDEX Grid_rows ON Grid(rows);
CREATE INDEX Grid_columns ON Grid(columns);
CREATE INDEX Grid_totalMines ON Grid(totalMines);

insertGrid:
INSERT INTO Grid (rows, columns,totalMines,startTime,endTime, grid)
VALUES (?,?,?,?,?,?);

getGridFor:
SELECT *
FROM Grid
WHERE rows = ?
AND columns = ?
AND totalMines = ?;